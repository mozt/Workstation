! Font
!URxvt.font: xft:Monospace:pixelsize=10
!URxvt.boldfont: xft:Monospace-Bold:pixelsize=10 
!URxvt.font:     xft:DejaVu Sans Mono:size=9:antialias=true:autohint=false:hinting=true
!URxvt.boldFont: xft:DejaVu Sans Mono:style=Bold:size=9:antialias=true:autohint=false:hinting=true
!URxvt.font:     xft:DejaVu Sans Mono:size=9:antialias=true
!URxvt.boldFont: xft:DejaVu Sans Mono:style=Bold:size=9:antialias=true

URxvt.font:     xft:Ubuntu Mono derivative Powerline:size=10:antialias=true
URxvt.boldFont: xft:Ubuntu Mono derivative Powerline:style=Bold:size=10:antialias=true

!Disable scrollbar
URxvt.scrollBar: false

! xterm: scrollback buffer size
URxvt*saveLines: 5000

! Transparency
URxvt.transparent: true
URxvt.shading: 30

! Addons
! URxvt.perl-ext-common:  default,tabbed
URxvt.perl-ext-common:  default

! Addon: Tabbed
! URxvt.tabbed.new-button: no
! URxvt.tabbed.tabbar-fg: 15
! URxvt.tabbed.tabbar-bg: 0
! URxvt.tabbed.tab-fg:    8
! URxvt.tabbed.tab-bg:    14

!URxvt.clipboard.autocopy: true

Xcursor.size: 16

!urxvt*termname:          xterm-256color
